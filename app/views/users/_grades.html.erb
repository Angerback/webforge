<%# Contiene el listado de calificaciones (historial) solamente. %>

<div id="user-info-extra" style="display:none">
<table class="table">

			<div align="center"><%= @evaluation.name %></div>
			<tr>
				<th>NÂ°</th>
				<th>Fecha</th>
				<th>Calificacion</th>
				<% count = 1 %>
			</tr>
			<% @userTests.each do |test| %>
				<tr>
					<td><%= count %></td>
					<td><%= test.created_at.strftime("%m/%d/%Y") %></td>
					<td><%= number_with_precision(test.grade, :precision =>2 ) %></td>
					<% count += 1 %>
				</tr>
			<% end %>
</table>
</div>
